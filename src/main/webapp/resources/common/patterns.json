{
    "property_patterns": [
        {
            "Response": {
                "name": "応答パターン",
                "intent": [
                    "ある要求(request)が成り立つとき、いずれは必ず応答(response)が成り立つ。"
                ],
                "templates": [
                    "〇〇のときはいつも、いずれは〇〇が成り立つ。"
                ]
            }
        },

        {
            "Universality": {
                "name": "普遍パターン",
                "intent": [
                    "常にconditionが成立する。"
                ],
                "templates": [
                    "〇〇が常に成立する。",
                    "ずっと〇〇である。",
                    "常に〇〇の状態である。"
                ]
            }
        },

        {
            "Absence": {
                "name": "不在パターン",
                "intent": [
                    "どんなときでもconditionが成り立つことはない。"
                ],
                "templates": [
                    "決して〇〇になることはない。",
                    "常に〇〇の状態になることはない。"
               	]
            }
        },

        {
            "Precedence": {
                "name": "先行パターン",
                "intent": [
                    "特定の状態Pが成り立つ前に必ず事前条件Sが成り立つ。"
                ],
                "templates": [
                    "〇〇が成り立つときは、必ず〇〇が成り立つ。",
                    "〇〇になるときには〇〇が必要である。"
                ]
            }
        }
    ]
}
