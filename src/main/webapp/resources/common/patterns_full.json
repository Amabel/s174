{
    "property_patterns": [
        {
            "Response": {
                "name": "応答パターン",
                "intent": [
                    "ある要求(request)が成り立つとき、いずれは必ず応答(response)が成り立つ。"
                ],
                "templates": [
                    "〇〇のときはいつも、いずれは〇〇が成り立つ。"
                ]
            }
        },

        {
            "Universality": {
                "name": "普遍パターン",
                "intent": [
                    "常にconditionが成立する。"
                ],
                "templates": [
                    "〇〇が常に成立する。",
                    "ずっと〇〇である。",
                    "常に〇〇の状態である。"
                ]
            }
        },

        {
            "Absence": {
                "name": "不在パターン",
                "intent": [
                    "どんなときでもconditionが成り立つことはない。"
                ],
                "templates": [
                    "決して〇〇になることはない。",
                    "常に〇〇の状態になることはない。"
               	]
            }
        },

        {
            "Precedence": {
                "name": "先行パターン",
                "intent": [
                    "特定の状態Pが成り立つ前に必ず事前条件Sが成り立つ。"
                ],
                "templates": [
                    "〇〇が成り立つときは、必ず〇〇が成り立つ。",
                    "〇〇になるときには〇〇が必要である。"
                ]
            }
        },

        {
            "Existence": {
                "name": "存在パターン",
                "intent": [
                    "いつかPが成立する。"
                ],
                "templates": [
                    "いつか〇〇の状態になる。",
                    "必ずどこかで〇〇が成立する。",
                    "一度は必ず〇〇が成立する。"
                ]
            }
        },

        {
            "Bounded-Existence": {
                "name": "有限存在パターン",
                "intent": [
                    "Pが最大2回まで成立する。"
                ],
                "templates": [
                    "〇〇が2回まで成立する。",
                    "〇〇が最大2度発生する。"
                ]
            }
        },

        {
            "2-1-Precedence-Chain": {
                "name": "連続先行パターン",
                "intent": [
                    "特定の状態Pが成り立つ前にかならず事前条件S, Tが成り立つ。"
                ],
                "templates": [
                    "〇〇が成り立つときは、必ず〇〇が成り立ち、さらにその前に〇〇が成り立つ。"
                ]
            }
        },

        {
            "1-2-Precedence-Chain": {
                "name": "連続先行パターン",
                "intent": [
                    "特定の状態S, Tが成り立つ前に必ず事前条件Pが成り立つ。"
                ],
                "templates": [
                    "〇〇と〇〇が成り立つときは、必ず〇〇が成り立つ",
                    "〇〇が最大2度発生する。"
                ]
            }
        },

        {
            "1-2-Response-Chain": {
                "name": "連続応答パターン",
                "intent": [
                    "ある要求Pが成り立つとき、いずれは必ず応答S, Tが成り立つ。"
                ],
                "templates": [
                    "〇〇が2回まで成立する。",
                    "〇〇が最大2度発生する。"
                ]
            }
        },

        {
            "2-1-Response-Chain": {
                "name": "連続応答パターン",
                "intent": [
                    "ある要求S, Tが成り立つとき、いずれは必ず応答Pが成り立つ。"
                ],
                "templates": [
                    "〇〇と〇〇のときはいつも、いずれは〇〇が成り立つ。"
                ]
            }
        }
    ]
}
